<html>
    <body>
        <h3>Student-Registration-Form</h3>
        <form action="StudentAccountCreation">
            <pre>
            Userid      <input type="text" name="uid"/>
            Password    <input type="password" name="pw"/>
            Name        <input type="text" name="name"/>
            Branch       <select name="branch">
                            <option>civil</option>
                            <option>mech</option>
                            <option>cs</option>
                            <option>ec</option>
                        </select>
            Sem         <select name="sem">
                        <option>1</option>
                        <option>2</option>
                        <option>3</option>
                        <option>4</option>
                        <option>5</option>
                        <option>6</option>
                        <option>7</option>
                        <option>8</option>
                        </select>
            <input type="submit" value="Create-Account"/>
            </pre>
        </form>
        <a href="adminhome.jsp">Home</a>
    </body>
</html>
